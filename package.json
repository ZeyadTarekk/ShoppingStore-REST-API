    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "pg": "^8.8.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.7.16",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
